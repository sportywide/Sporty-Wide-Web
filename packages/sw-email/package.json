{
	"name": "sportywide-email",
	"version": "1.0.0",
	"description": "Email microservice",
	"license": "ISC",
	"scripts": {
		"build": "NODE_ENV=production webpack -r sportywide-shared/build/ts-node/register -r tsconfig-paths/register",
		"dev": "rm -rf dist/ && run-p dev:*",
		"dev:server": "wait-on dist/main.js && nodemon",
		"dev:webpack": "webpack -r sportywide-shared/build/ts-node/register -r tsconfig-paths/register"
	},
	"dependencies": {
		"nodemailer": "6.3.0",
		"sportywide-core": "1.0.0",
		"sportywide-schema": "1.0.0",
		"sportywide-shared": "1.0.0"
	}
}
