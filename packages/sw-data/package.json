{
	"name": "sportywide-data",
	"version": "1.0.0",
	"description": "Data Crawler service for SportyWide",
	"license": "ISC",
	"scripts": {
		"build": "TS_NODE_TYPE_CHECK=false TS_NODE_TRANSPILE_ONLY=true NODE_ENV=production webpack -r tsconfig-paths/register",
		"dev:build": "TS_NODE_TYPE_CHECK=false TS_NODE_TRANSPILE_ONLY=true webpack -r tsconfig-paths/register",
		"dev:exec": "npm run dev:build --script=${npm_config_script} && node dist/${npm_config_script}",
		"exec": "node dist/${npm_config_script}"
	},
	"dependencies": {
		"cheerio": "0.22.0",
		"sportywide-core": "1.0.0",
		"sportywide-schema": "1.0.0",
		"sportywide-shared": "1.0.0"
	},
	"devDependencies": {
		"@types/cheerio": "0.22.13"
	}
}
