{
	"name": "sportywide-web",
	"version": "1.0.0",
	"description": "",
	"author": "",
	"license": "MIT",
	"scripts": {
		"dev": "NODE_ENV=development nodemon -L",
		"build": "next build ./src",
		"start": "NODE_ENV=production ts-node -r tsconfig-paths/register ./src/next-server.ts",
		"analyze": "BUNDLE_ANALYZE=both next build ./src",
		"analyze:server": "BUNDLE_ANALYZE=server next build ./src",
		"analyze:browser": "BUNDLE_ANALYZE=browser next build ./src"
	},
	"dependencies": {
		"axios": "0.19.0",
		"axios-observable": "1.1.2",
		"express": "4.17.1",
		"hoist-non-react-statics": "3.3.0",
		"http-proxy-middleware": "0.19.1",
		"next": "9.0.3",
		"next-redux-wrapper": "2.1.0",
		"prop-types": "15.7.2",
		"react": "16.8.6",
		"react-dom": "16.8.6",
		"react-redux": "7.1.0",
		"recompose": "0.30.0",
		"redux": "4.0.4",
		"redux-devtools-extension": "2.13.8",
		"redux-observable": "1.1.0",
		"redux-thunk": "2.3.0",
		"rxjs": "6.5.2",
		"semantic-ui-react": "0.87.3",
		"sportywide-shared": "1.0.0",
		"sporty-wide-style": "1.0.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
